import java.io.*;
public class Type {
public static void main( String args[]) throws Exception {
FileReader fr=new FileReader(args[0]);
PrintWriter pw=new PrintWriter(System.out,true);
char c[]=new char[4096];
int read=0;
while ((read=fr.read(c)) != -1) pw.write(c,0,read);
fr.close();
pw.close();
}
}
